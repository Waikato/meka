# Parameters for the GUI.
#
# Author:  FracPete (fracpete at waikato dot ac dot nz)
# Version: $Revision: 11598 $

# Whether to use the frame maximization fix (affects only Linux) (true|false)
UseFrameMaximizationFix=false

# The delay in msec used by the frame maximization fix
FrameMaximizationFixDelay=200

# the default sizes for the file choosers
FileChooser.Width=750
FileChooser.Height=500

# the default width/height for applications
Default.Width=800
Default.Height=600

# the GUIChooser window
meka.gui.guichooser.GUIChooser.Pack=true

# the Explorer window
meka.gui.explorer.Explorer.Icon=explorer.gif
meka.gui.explorer.Explorer.Width=800
meka.gui.explorer.Explorer.Height=600
meka.gui.explorer.Explorer.Pack=false

# the Arff viewer window
weka.gui.arffviewer.ArffViewerMainPanel.Icon=report.gif
weka.gui.arffviewer.ArffViewerMainPanel.Width=1024
weka.gui.arffviewer.ArffViewerMainPanel.Height=768
weka.gui.arffviewer.ArffViewerMainPanel.Pack=false
